<!DOCTYPE html>
<html>

  <head>

    {{template "head"}}

  </head>

  <body>

    {{template "navBar"}}

    <div class="clr"></div>

    <section>
      <div class="container">
        <div class="table-title">
          <h3>Latest Blocks</h3>
        </div>
        <div class="data-table" id="block-table">
          <table class="table">
            <thead>
              <tr id="header-row">
                <th>Hash</th>
                <th style="width: 20%;">Timestamp</th>
                <th style="width: 10%;">NrFundsTX</th>
                <th style="width: 10%;">NrAccTX</th>
                <th style="width: 10%;">NrConfigTX</th>
                <th style="width: 10%;">NrStakeTX</th>
                <th style="width: 10%;">NrUpdateTX</th>
              </tr>
            </thead>
            <tbody>
              {{range .Blocks}}
              <tr>
                <td> <a href="../block/{{.Hash}}">{{.Hash}}</a></td>
                <td>{{.TimeString}}</td>
                <td>{{.NrFundsTx}}</td>
                <td>{{.NrAccTx}}</td>
                <td>{{.NrConfigTx}}</td>
                <td>{{.NrStakeTx}}</td>
                <td>{{.NrUpdateTx}}</td>
              </tr>
              {{end}}
            </tbody>
          </table>
        </div>
      </div>
    </section>

    {{template "scriptImports"}}

  </body>
</html>
