<!DOCTYPE html>
<html>

  <head>

    {{template "head"}}

  </head>

  <body>

    {{template "navBar"}}

    <div class="clr"></div>

    <section>
      <div class="container">
        <div class="table-title">
          <h3>Transaction {{.Hash}}</h3>
        </div>
        <div class="data-table" id="transactions-table">
          <table class="table">
            <thead>
              <tr>
                <th style="width: 30%;">Hash</th>
                <td>{{.Hash}}</td>
              </tr>
              <tr>
                <th>Block</th>
                <td> <a href="{{.UrlLevel}}/block/{{.BlockHash}}">{{.BlockHash}}</a></td>
              </tr>
              <tr>
                <th>Tx to update</th>
                <td> <a href="{{.UrlLevel}}/tx/acc/{{.ToUpdateHash}}">{{.ToUpdateHash}}</a></td>
              </tr>
              <tr>
                <th>Data to be updated</th>
                <td>{{.ToUpdateData}}</td>
              </tr>
              <tr>
                <th>Fee</th>
                <td>{{.Fee}}</td>
              </tr>
              <tr>
                <th>Data</th>
                <td>{{.Data}}</td>
              </tr>
              <tr>
                <th>Signature</th>
                <td>{{.Signature}}</td>
              </tr>
            </thead>

          </table>
        </div>
      </div>
    </section>

    {{template "scriptImports"}}

  </body>
</html>
